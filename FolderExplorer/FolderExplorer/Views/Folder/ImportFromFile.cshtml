@model FolderExplorer.Entities.Folder

<h2>Import</h2>

<form method="post" enctype="multipart/form-data" asp-action="ImportFromFile">
    <div class="form-group">
        <label for="file">Choose JSON file:</label>
        <input type="file" name="file" id="file" class="form-control" required>
    </div>
    <button type="submit" class="btn btn-primary">Import</button>
</form>

<a asp-controller="Folder" asp-action="ShowFolder">Back to Folders</a>